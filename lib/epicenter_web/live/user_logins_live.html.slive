section#user-logins-page data-page="user-logins"
  h2
    #title
      span data-role="title"
        = "Audit trail for #{@user.name}"

  table data-role="logins-table"
    thead
      tr data-role="table-column-names"
        th Timestamp
        th OS
        th Browser
        th Session ID
    tbody
      = for login <- @logins do
        tr
          td *
          td *
          td *
          td= login.session_id
