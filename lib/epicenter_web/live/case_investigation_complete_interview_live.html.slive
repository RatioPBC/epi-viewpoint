#case-investigation-complete-page data-page="case-investigation-complete"

  section
    header#subnav
      = live_redirect to: Routes.profile_path(EpicenterWeb.Endpoint, EpicenterWeb.ProfileLive, @person), data: [confirm: @confirmation_prompt, role: "back-link"] do
        = back_icon()
        span Back

  = form_for @form_changeset, "#", [id: "case-investigation-interview-complete-form", phx_change: "change", phx_submit: "save"], fn f ->
    = complete_interview_form_builder(f)